{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "Smootheez"
  ],
  "license": "${mod_license}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.modid.ModCommon"
    ],
    "client": [
      "com.example.modid.ModClient"
    ]
  },
  "mixins": [
    "modid.mixins.json",
    {
      "config": "modid.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=${minecraft_version}"
  }
}